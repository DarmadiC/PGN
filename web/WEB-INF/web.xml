         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>loginhandler</servlet-name>
        <servlet-class>loginhandler</servlet-class>
    </servlet>
    <servlet-mapping>
